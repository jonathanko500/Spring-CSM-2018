public class M1
{//start class

	public static void main(String[] args)
	{//start main
		int a=3,b=4,c=2,d=7;
		double ans;
		ans=calculateMoney(a,b,c,d);
		System.out.println(ans);
	}//end main
	public static double calculateMoney(int numQuarters, int numDimes, int numNickels, int numPennies){

		double quarter=0.25, dime=0.1,nickel=0.05,penny=0.01,total;

		total=(numQuarters*quarter)+(numDimes*dime)+(numNickels*nickel)+(numPennies*penny);
		return total;
		}
}//end class
