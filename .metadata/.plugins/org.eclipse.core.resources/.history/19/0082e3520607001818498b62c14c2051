
public class M2Prac 
{//start class

	public static void main(String[] args) 
	{//start main
		int[] list = new int[]{1, 5, 2, 4, 7, 3};
		System.out.println(subArray(list,1,4));
		
	}//end main
	public static int[] subArray(int[] numbers, int start, int end)
	{
		int[] newNumb = new int[numbers.length];
		for(int i=0;i<numbers.length;i++)
		{
			if(numbers[i]!=start||numbers[i]!=end)
			{
				newNumb[i]=numbers[i];
			}
		}
		return newNumb;
		
	}
}//end class
