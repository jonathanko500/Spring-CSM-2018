
public class M2Prac 
{//start class

	public static void main(String[] args) 
	{//start main
		String[] list = new String[]{"hello","bye"};
		convertToUpper(list);
		
	}//end main
	public static void convertToUpper(String[] words)
	{
		
		for(int i=0;i<words.length;i++)
		{
			System.out.println(words[i].toUpperCase());
		}
	}
}//end class
