	public static void main(String[] args) 
	{
		LinkedList<Integer> q = new LinkedList<Integer>();

		q.offer(4);

		q.offer(1);

		System.out.println(q.poll());   // line 1- what is dequeued?

		q.offer(2);

		q.offer(3);

		q.offer(5);

		q.offer(8);

		System.out.println(q.poll());   // line 2- what is dequeued?

		System.out.println(q.poll());   // line 3- what is dequeued?

		System.out.println(q.poll());   // line 4- what is dequeued?

		q.offer(6);

		q.offer(7);

		System.out.println(q.poll());   // line 5- what is dequeued?

		q.offer(q.poll());           

		System.out.println(q.poll());;   // line 6- what is dequeued?

		System.out.println(q.poll());   // line 7- what is dequeued?

		System.out.println(q.poll());;   // line 8- what is dequeued?
	}
	

}//end class
