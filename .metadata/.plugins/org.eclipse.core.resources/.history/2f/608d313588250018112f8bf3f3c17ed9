//M7Test
public class Bird extends Pet
{//start class
	private boolean sing;
	//constructor
	public Bird(String name, int age, boolean sing) {
		super(name, age);
		this.sing=sing;
	}
	@Override
	public void printVetAppointmentReminder() {
		System.out.println("You have an appointment with a vet today.");
	}
}//end classs