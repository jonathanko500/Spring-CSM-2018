import java.util.*;
import java.io.*;

public class M9Prac 
{//start class

	public static void main(String[] args) throws IOException 
	{
		//first read input
		Scanner fileScan = new Scanner(new FileReader(new File("EX.txt")));
		while(fileScan.hasNext())
		{
			String fileLine=fileScan.nextLine();
			System.out.println(fileLine);
		}
	}

}//end class
