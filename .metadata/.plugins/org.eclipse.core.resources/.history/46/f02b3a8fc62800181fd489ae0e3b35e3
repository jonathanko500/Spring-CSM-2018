import java.util.Random;

public class MIdterm
{//start class

	public static void main(String[] args)
	{
		int[] list = new int[]{3,17,2,22,9,7,6};
		String[] list2 = new String[]{"Hello","Bye","Tie","energetic"};
		/*
		for(int i=0;i<list.length;i++)
		{
			System.out.println(list[i]);
			list[i]++;
			System.out.println(list[i]);
			System.out.println("-----------");
		}
		*/
		System.out.println(countWordsThatContainChar(list2,'e'));

	}
	public static boolean isPositiveAndMultipleThree(int x)
	{//start method
		if(x>0)
		{
			if(x%3==0)
			{
				return true;
			}
			else
			{
				return false;
			}
		}
		else
		{
			return false;
		}
	}//end method
	public static boolean isPositiveAndMultipleThree(int x,int y)
	{//start method
		if(x>0 && y>0)
		{
			if(x%3==0 && y%3==0)
			{
				return true;
			}
			else
			{
				return false;
			}
		}
		else
		{
			return false;
		}
	}//end method
	public static int countWordsThatContainChar(String[] list,char x)
	{
		int y=0;
		for(int i=0;i<list.length;i++)
		{
			for(int j=0;j<list[i].length();j++)
			{
				if(list[i].indexOf(x)>0)
				{
					y++;
				}
			}
		}
		return y;
	}
}//end class
