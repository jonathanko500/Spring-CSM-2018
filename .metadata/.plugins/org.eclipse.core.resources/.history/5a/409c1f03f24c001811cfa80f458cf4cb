
public class M13Teest {

	public static void main(String[] args) 
	{
		System.out.println(mysteryMethod("poop",'p',0));

	}
	public static  int mysteryMethod(String a, char b, int i) {
		
		if (i == a.length(  ))

			return 0;

			else if (b == a.charAt(i))

			return 1;

			else

			return mysteryMethod(a, b, i+1);
	}
}
