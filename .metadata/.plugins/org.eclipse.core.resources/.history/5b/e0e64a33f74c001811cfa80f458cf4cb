
public class M13Teest {

	public static void main(String[] args) 
	{
		int[] a = {10, 3, 6, 2, 1, 4};
		printMultiples(a,2);
		
	}
	//25
	public static void printMultiples(int[] nums, int multi)
	{
		printMultiplesH(nums,multi,0);
	}
	//26
	private static void printMultiplesH(int[] nums, int multi, int place)
	{
		if(nums[place]%multi==0)
		{
			System.out.println(nums[place]);
		}
		else
		{
			printMultiplesH(nums,multi,place+1);
		}
	}
	public static String reverseString(String originalString)
	{
		String x="";
		
		return x;
	}
	//27
	
}
