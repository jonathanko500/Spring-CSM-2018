import java.util.Random;

public class M2Test
{
	public static void main(String[] args)
	{//start main
		int[] Ex= new int[]{1,2,3,4,5};
		int[] newArr = new int[]{10,20,30,40,50};
		switchThem(Ex,newArr);
	}//end main
	public static void switchThem(int[] first, int[] second)
	{//start method
		int start=second.length-1;
		for(int i=0;i<first.length;i++)
		{
			first[i]=second[start];
			second[i]=first[start];
			start--;
		}
		
	}//end method
}
