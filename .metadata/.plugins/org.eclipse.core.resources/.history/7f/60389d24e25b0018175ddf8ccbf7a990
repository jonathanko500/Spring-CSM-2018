public class Final 
{//start class extends Application
	/*
	public void start(Stage arg0)
	{
		
	}
	*/
	public static void main(String[] args) 
	{
		printPositiveAverage(2,3);
	}
	public static void printPositiveAverage(int x, int y, int z) 
	{
		double avg=(x+y+z)/3;
		System.out.println(avg);
	}

}//end class
