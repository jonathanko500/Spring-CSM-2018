import java.util.Random;

public class MIdterm
{//start class

	public static void main(String[] args)
	{
		String a = "3", b = "4";

		int y = 6, z = 7;

		System.out.println(y + z + a + b);


	}
	public static boolean isPositiveAndMultipleThree(int x)
	{//start method
		if(x>0)
		{
			if(x%3==0)
			{
				return true;
			}
			else
			{
				return false;
			}
		}
		else
		{
			return false;
		}
	}//end method
	public static boolean isPositiveAndMultipleThree(int x,int y)
	{//start method
		if(x>0 && y>0)
		{
			if(x%3==0 && y%3==0)
			{
				return true;
			}
			else
			{
				return false;
			}
		}
		else
		{
			return false;
		}
	}//end method
}//end class
