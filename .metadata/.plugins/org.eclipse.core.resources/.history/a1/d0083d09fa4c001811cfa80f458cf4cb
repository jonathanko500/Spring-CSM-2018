
public class M13Teest {

	public static void main(String[] args) 
	{
		System.out.println(reverseString("THOMAS"));	
	}
	//25
	public static void printMultiples(int[] nums, int multi)
	{//start
		printMultiplesH(nums,multi,0,nums.length-1);
	}//end
	private static void printMultiplesH(int[] nums, int multi, int first ,int last)
	{//start
		if(first==last)
		{
			if((nums[first]%multi)==0)
			{
				System.out.println(nums[first]);
			}
		}
		else
		{
			if((nums[first]%multi)==0)
			{
				System.out.println(nums[first]);
			}
			printMultiplesH(nums,multi,first+1,last);
		}
	}//end
	//26
	public static String reverseString(String originalString)
	{
		String x=reverseStringH(originalString,0,originalString.length()-1);
		return x;
	}
	private static String reverseStringH(String originalString, int begin,int end)
	{
		String x="";
		if(begin==end)
		{
			x+=originalString.charAt(end);
		}
		else
		{
			x+=originalString.charAt(end)+reverseStringH(originalString, begin,end-1);
			
		}
		return x;
	}
	//27
	
}
