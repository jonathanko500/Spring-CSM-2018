	public static void main(String[] args) 
	{
		printStars(5);
		printStars2(5);
	}
	public static void printStars(int n)
	{

		for(int i=0; i<=n; i++) {

		System.out.println("*");

		}
	}
	public static void printStars2(int x)
	{
		if(x==0)
		{
			System.out.println("*");
		}
		else
		{
			System.out.println("*");
			printStars(x-1);
		}
	}
}//end class
