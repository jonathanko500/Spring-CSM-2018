import java.util.Random;

public class M2Test {

	public static void main(String[] args)
	{//start main
		String[] word = new String[]{"alaska","florida","iowa","nevada","wyoming"};
		for(int i=0;i<word.length;i++)
		{//start loop
			System.out.println(word[i]);
		}//end loop
		System.out.println("----------");
		String[] newWord = new String[10];
		for(int i=0;i<newWord.length;i++)
		{//start loop
			switch(i)
			{
			case 0:
			case 1:
			case 2:
			case 3:
			case 4:
				newWord[i]=word[i];
				break;
			case 5:
				newWord[i]="California";
				break;
			case 6:
				newWord[i]="North Dakota";
				break;
			case 7:
				newWord[i]="South Dakota";
				break;
			case 8:
				newWord[i]="Washington";
				break;
			case 9:
				newWord[i]="Oregon";
				break;
			}
			System.out.println(newWord[i]);
		}
	}//end main
	public static String[] Upper(String[] change)
	{//start method
		String[] word = new String[change.length];
		for(int i=0;i<change.length;i++)
		{//start loop
			word[i]=change[i].toUpperCase();
		}//end loop
		return word;
	}//end method
}
