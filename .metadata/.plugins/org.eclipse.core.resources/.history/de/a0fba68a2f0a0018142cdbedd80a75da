import java.util.Random;

public class M2Test
{
	public static void main(String[] args)
	{//start main
		int[] EX = new int[]{7,9,45,32,-1,74,103,-1874};
		double numb=calculateAverage(EX,5);
		System.out.println(numb);
	}//end main
	public static double calculateAverage(int[] nums, int numElements)
	{//start method
		double avg=0;
		for(int i=0;i< numElements;i++)
		{
			avg+=nums[i];
		}
		avg/= numElements;
		return avg;
	}//end method
}
