import javafx.application.*;
import javafx.geometry.*;
import javafx.scene.*;
import javafx.scene.image.*;
import javafx.scene.layout.*;
import javafx.scene.paint.*;
import javafx.scene.shape.*;
import javafx.scene.text.*;
import javafx.stage.*;
public class M10PracV2 
{//start
	public void start(Stage primaryStage)
	{//start class
		int x=100,y=100,length=200,width=50;
		VBox primaryBox = new VBox();
		Group group= new Group();
		for(int i=0;i<10;i++)
		{
			Rectangle s= new Rectangle(x,y,length,width);
			s.setTranslateX(i*10);
			s.setTranslateY(i*10);
			s.setFill(Color.TRANSPARENT);
			s.setStroke(Color.BLACK);
			group.getChildren().add(s);
		}
		Pane pane = new FlowPane(group);
		Scene z = new Scene (pane,300,300);
		primaryStage.setScene(z);
		primaryStage.setTitle("Transform");
		primaryStage.show();
	}//end class

	public static void main(String[] args) 
	{//start
		
	}//end main
}//end
