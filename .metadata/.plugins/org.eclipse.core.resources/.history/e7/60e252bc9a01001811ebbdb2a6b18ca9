public class M1
{//start class

	public static void main(String[] args)
	{//start main
		int limit = 100, num1 = 15, num2 = 40;

		if( limit <= limit) {
		    if( num1 == num2) {
		      System.out.print("lemon ");
		    }
		    System.out.print("lime ");
		} 
		System.out.print("grape ");
	}//end main
	
}//end class
