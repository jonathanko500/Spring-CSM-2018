import javafx.application.*;
import javafx.geometry.*;
import javafx.scene.*;
import javafx.scene.image.*;
import javafx.scene.layout.*;
import javafx.scene.paint.*;
import javafx.scene.shape.*;
import javafx.scene.text.*;
import javafx.stage.*;

public class M10Prac extends Application
{//start class
	
	public void start(Stage primaryStage)
	{
		VBox primaryBox = new VBox();
		/*
		
		DOG PIC
		primaryBox.setAlignment(Pos.CENTER);
		Image pic = new Image("dog1.jpg",500,300,true,true);
		ImageView picView = new ImageView(pic);
		primaryBox.getChildren().add(picView);
		
		Text write = new Text("LAZY DOG");
		write.setFont(Font.font(50));
		primaryBox.getChildren().add(write);
		
		Scene scene = new Scene(primaryBox,700,700,Color.WHITE);
		primaryStage.setTitle("GUI Image");
		primaryStage.setScene(scene);
		primaryStage.show();
		*/
		
		int x=100,y=100,length=200,width=50;
		
		Group group= new Group();
		for(int i=0;i<10;i++)
		{
			Rectangle s= new Rectangle(x,y,length,width);
			s.setTranslateX(i*10);
			s.setTranslateY(i*10);
			s.setFill(Color.TRANSPARENT);
			s.setStroke(Color.BLACK);
			group.getChildren().add(s);
			
		}
	}
	
	
	
	
	public static void main(String[] args)
	{
		launch(args);
	}
}//end class
