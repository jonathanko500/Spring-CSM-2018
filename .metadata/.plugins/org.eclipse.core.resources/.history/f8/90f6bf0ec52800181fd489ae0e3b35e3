import java.util.Random;

public class MIdterm
{//start class

	public static void main(String[] args)
	{
		String text = "Learning Java is fun.";

		String result = text.substring(3, 7);
		System.out.println(result);


	}
	public static boolean isPositiveAndMultipleThree(int x)
	{//start method
		if(x>0)
		{
			if(x%3==0)
			{
				return true;
			}
			else
			{
				return false;
			}
		}
		else
		{
			return false;
		}
	}//end method
	public static boolean isPositiveAndMultipleThree(int x,int y)
	{//start method
		if(x>0 && y>0)
		{
			if(x%3==0 && y%3==0)
			{
				return true;
			}
			else
			{
				return false;
			}
		}
		else
		{
			return false;
		}
	}//end method
}//end class
